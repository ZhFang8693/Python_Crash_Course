my_favor_fuit = ['orange','grape','watermelon']

if 'apple' in my_favor_fuit:
    print ("No,apple not my favor")
if 'orange' in my_favor_fuit:
    print ("Yes,orange!that's my favor")
if 'banana' in my_favor_fuit:
    print ("No,banana not my favor")
if 'grape' in my_favor_fuit:
    print ("Yes,grape!that's my favor")

    